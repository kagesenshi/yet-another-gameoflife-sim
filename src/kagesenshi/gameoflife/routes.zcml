<configure xmlns="http://pylonshq.com/pyramid"
        xmlns:zcml="http://namespaces.zope.org/zcml">
    <include package="pyramid_zcml"/>

    <route 
        name="gameoflife"
        pattern="/gameoflife"/>

    <route 
        name="golsession"
        pattern="golsession/{sessionid}.json"/>

    <route 
        name="shorturl"
        pattern="/shorturl"/>

    <route 
        name="resolve_url"
        pattern="/s/{short_id}"/>

</configure>
